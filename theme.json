{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "607px",
			"wideSize": "1000px"
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#4a7c59",
					"name": "Primary Green"
				},
				{
					"slug": "secondary",
					"color": "#6b9279",
					"name": "Secondary Green"
				},
				{
					"slug": "background",
					"color": "#D5DADD",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#333333",
					"name": "Foreground"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "light-gray",
					"color": "#f5f5f5",
					"name": "Light Gray"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.25rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "1.5rem",
					"name": "Extra Large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"slug": "system",
					"name": "System Font"
				},
				{
					"fontFamily": "Georgia, serif",
					"slug": "serif",
					"name": "Serif"
				}
			]
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true,
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/image": {
				"border": {
					"radius": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "2.5rem",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2rem",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem",
					"lineHeight": "1.4"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.5rem",
					"lineHeight": "1.4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem",
					"lineHeight": "1.5"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1rem",
					"lineHeight": "1.5"
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "3px"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--light-gray)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "1rem",
						"right": "1rem",
						"bottom": "1rem",
						"left": "1rem"
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "0 0 0 4px",
					"color": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"padding": {
						"left": "1.5rem"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		}
	]
}
